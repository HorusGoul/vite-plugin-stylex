{
  "name": "vite-plugin-stylex",
  "version": "0.9.0-next.1",
  "description": "Vite Plugin for StyleX",
  "author": "Horus Lugo <hola@horus.dev>",
  "license": "MIT",
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "vite",
    "stylex",
    "css-in-js",
    "plugin"
  ],
  "repository": "https://www.github.com/HorusGoul/vite-plugin-stylex",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
