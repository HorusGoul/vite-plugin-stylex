  "devDependencies": {
    "@types/babel__core": "^7.20.5",
    "@types/node": "20",
    "typescript": "5.2.2",
    "vite": "^5.2.7"
  },
  "dependencies": {
    "@babel/core": "^7.23.9",
    "@babel/plugin-syntax-flow": "^7.23.3",
    "@babel/plugin-syntax-jsx": "^7.23.3",
    "@babel/plugin-syntax-typescript": "^7.23.3",
    "@stylexjs/babel-plugin": "^0.5.1",
    "compare-versions": "^6.1.0"
  },
  "peerDependencies": {
    "@stylexjs/stylex": "^0.5.1",
    "vite": "^5.2.7"
